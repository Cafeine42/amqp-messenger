{
    "name": "omnilog/amqp-messenger-extended",
    "type": "symfony-bundle",
    "description": "Symfony AMQP extension Messenger Bridge with Consume method",
    "license": "MIT",
    "authors": [
        {
            "name": "Thibaut Cholley",
            "email": "thibaut.cholley@omnilog.fr"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-amqp": "*",
        "symfony/deprecation-contracts": "^2.1",
        "symfony/messenger": "^4.4"
    },
    "require-dev": {
        "symfony/event-dispatcher": "^4.4|^5.0",
        "symfony/process": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/serializer": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": { "Omnilog\\AmqpMessengerBundle\\": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
